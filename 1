def find_duplicates(lst):
    freq = {}          # dictionary to store frequency
    duplicates = []

    for item in lst:
        if item in freq:
            freq[item] += 1
        else:
            freq[item] = 1

    for key, value in freq.items():
        if value > 1:
            duplicates.append(key)

    return duplicates


# Example
numbers = [1, 2, 3, 2, 4, 5, 1, 6, 3]
print(find_duplicates(numbers))
